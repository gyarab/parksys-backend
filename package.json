  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "nconf": "^0.10.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.19.6",
    "sequelize-graphql-schema": "^0.1.70",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
