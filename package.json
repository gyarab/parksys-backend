    "@types/node": "^12.7.11",
    "@types/sequelize": "^4.28.5",
    "@types/socket.io": "^2.1.3",
    "concurrently": "^4.1.2",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "nodemon": "^1.19.3",
    "prettier": "^1.18.2",
    "sequelize-typescript": "^1.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
